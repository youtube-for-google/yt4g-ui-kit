{
  "compilerOptions": {
    "jsx": "react-jsx",
    "module": "ESNext",
    "target": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "strict": true,
    "declaration": true,
    "emitDeclarationOnly": false,
    "outDir": "dist",
    "skipLibCheck": true
  },
  "include": ["src/**/*", "vite-env.d.ts"]
}
